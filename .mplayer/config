[defaults]
profile-desc="Video defaults"
use-filedir-conf="yes"
msgcolor="yes"
fs="no"
osdlevel="2"
framedrop="no"
af="scaletempo"
vf="eq"
slang="en"
alang="en"

[cache]
profile-desc="Video stream caching"
msgcolor="yes"
cache=16384
cache-min=25
cache-seek-min=35

[dvd]
profile-desc="dvd streams"
msgcolor="yes"
vf=pp=hb/vb/dr/al/fd
alang=en
slang="en"

[dvdnav]
profile-desc="dvdnav streams"
profile=dvd
msgcolor="yes"
mouse-movements=yes
nocache=yes

